generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model bank {
  id       BigInt  @id @default(autoincrement())
  CID      String  @db.VarChar(13)
  IDBANK   String  @default("") @db.VarChar(255)
  NAMEBANK String? @db.VarChar(255)

  @@index([CID], map: "cid")
}

model cheque {
  ID         BigInt    @id @default(autoincrement())
  CHEQUE     String    @unique(map: "cheque") @db.VarChar(11)
  CHEQUENAME String    @db.VarChar(255)
  ACCNUMBER  String    @db.VarChar(10)
  PAYDATE    DateTime? @db.Date
  DUPDATE    DateTime  @db.DateTime(0)
}

model cmonth {
  ID           String @id @unique(map: "id") @db.VarChar(2)
  NAMEMONTH_TH String @db.VarChar(255)
  NAMEMONTH_EN String @db.VarChar(255)
}

model cpay {
  IDPAY   String  @id @db.VarChar(5)
  PAYNAME String? @db.VarChar(255)
  PAYTYPE String? @db.Char(1)

  @@index([IDPAY], map: "idpay")
}

model deegar {
  ID        BigInt   @id @default(autoincrement())
  PNUMBER   String   @db.VarChar(10)
  NODEEGAR  String   @default("1") @db.VarChar(4)
  ACCNUMBER String   @db.VarChar(10)
  ACCNAME   String   @db.VarChar(255)
  TAX       Decimal  @default(0.00) @db.Decimal(10, 2)
  FEE       Decimal  @default(0.00) @db.Decimal(10, 2)
  MONEY     Decimal  @default(0.00) @db.Decimal(10, 2)
  CHEQUE    String?  @db.VarChar(11)
  DUPDATE   DateTime @db.DateTime(0)

  @@unique([PNUMBER, NODEEGAR], map: "PNUMBER")
}

model officer {
  OFFICERID BigInt    @default(autoincrement())
  CID       String    @id @unique(map: "idp") @db.VarChar(13)
  IDCOOP    String?   @db.VarChar(5)
  IDMAN     String?   @db.VarChar(10)
  NO        String?   @db.VarChar(10)
  CODE      String?   @default("000") @db.VarChar(10)
  ID        String?   @db.VarChar(10)
  NAME      String?   @db.VarChar(255)
  SEX       String?   @db.VarChar(1)
  LPOS      String?   @db.VarChar(255)
  DUPDATE   DateTime? @db.DateTime(0)
  MOBILE    String?   @db.VarChar(10)
  EMAIL     String?   @db.VarChar(100)

  @@index([CODE], map: "code")
  @@index([OFFICERID], map: "officerid")
}

