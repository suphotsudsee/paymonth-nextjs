model regisdeegar {
  ID          BigInt    @id @default(autoincrement())
  PNUMBER     String    @unique(map: "PNUMBER") @db.VarChar(10)
  REGISDATE   DateTime  @db.Date
  NAME        String    @db.VarChar(255)
  MONEY       Decimal   @default(0.00) @db.Decimal(10, 2)
  TAX         Decimal   @default(0.00) @db.Decimal(10, 2)
  MONEYDRAW   Decimal   @default(0.00) @db.Decimal(10, 2)
  SENDDATE    DateTime? @db.Date
  GFMISNUMBER String    @db.VarChar(10)
  BANKDATE    DateTime? @db.Date
  MONEYBANK   Decimal?  @default(0.00) @db.Decimal(10, 2)
  GFMISREGIS  String?   @db.VarChar(10)
  GFMISDATE   DateTime? @db.Date
  CODEBUDGET  String    @db.VarChar(16)
  CODEACTIVE  String    @db.VarChar(5)
  DUPDATE     DateTime  @db.DateTime(0)
}

model salary {
  ID        BigInt   @unique(map: "ID") @default(autoincrement())
  CID       String   @db.VarChar(13)
  IDPAY     String   @db.VarChar(5)
  BANKID    BigInt?  @db.BigInt
  PNUMBER   String   @default("P000000000") @db.Char(10)
  NODEEGAR  String   @default("1") @db.VarChar(4)
  NUM       String   @default("1") @db.Char(1)
  MONTHTHAI String   @db.VarChar(2)
  YEARTHAI  String   @db.VarChar(4)
  MONEY     Decimal  @default(0.00) @db.Decimal(10, 2)
  DUPDATE   DateTime @db.DateTime(0)

  @@unique([CID, IDPAY, PNUMBER, NODEEGAR, NUM, MONTHTHAI, YEARTHAI], map: "IDPYEAR")
}

model station {
  CODE        String  @id @unique(map: "code") @db.VarChar(3)
  DEPART      String? @db.VarChar(255)
  CODEPLACE   String? @db.VarChar(5)
  NAMESTATION String? @db.VarChar(255)
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model user {
  id          Int    @id @default(autoincrement())
  username    String @default("") @db.VarChar(50)
  password    String @default("") @db.VarChar(50)
  cid         String @db.VarChar(13)
  fname       String @default("") @db.VarChar(255)
  lname       String @default("") @db.VarChar(50)
  status      String @default("") @db.VarChar(255)
  accessLevel Int    @default(0)
  mobile      String @default("") @db.VarChar(50)
  email       String @default("") @db.VarChar(100)
}
